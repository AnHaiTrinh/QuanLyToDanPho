<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPaneChinhCanBo" prefHeight="603.0" prefWidth="695.0"
            stylesheets="@Css/GiaiThuongDipDacBietCanBoView.css" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.se07.controller.controllers.controllerscanbo.ControllerGiaiThuongDipDacBietCanBoView">
    <children>
        <ImageView fx:id="imageLeftAdmin" fitHeight="608.0" fitWidth="230.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@Icon/imageAdmin.jpg"/>
            </image>
        </ImageView>
        <Button fx:id="buttonTrangChuCanBo" mnemonicParsing="false" onMousePressed="#onPressedButtonTrangChuCanBo"
                prefHeight="66.0" prefWidth="222.0" text="Trang chủ" textFill="WHITE">
            <graphic>
                <ImageView fitHeight="32.0" fitWidth="34.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@Icon/IconAadminHome.png"/>
                    </image>
                </ImageView>
            </graphic>
            <font>
                <Font name="System Bold" size="20.0"/>
            </font>
        </Button>
        <Button fx:id="buttonNhanKhauCanBo" layoutY="121.0" mnemonicParsing="false"
                onMousePressed="#onPressedButtonNhanKhauCanBo" prefHeight="58.0" prefWidth="222.0" text="Nhân khẩu"
                textFill="#f2f2f2">
            <graphic>
                <ImageView fitHeight="45.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@Icon/IconAdminPeople.png"/>
                    </image>
                </ImageView>
            </graphic>
            <font>
                <Font name="System Bold" size="18.0"/>
            </font>
        </Button>
        <Button fx:id="buttonHoKhauCanBo" layoutY="211.0" mnemonicParsing="false"
                onMousePressed="#onPressedButtonHoKhauCanBo" prefHeight="58.0" prefWidth="222.0" text="Hộ khẩu"
                textFill="#f2f2f2">
            <graphic>
                <ImageView fitHeight="46.0" fitWidth="53.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@Icon/IconAdminUser.png"/>
                    </image>
                </ImageView>
            </graphic>
            <font>
                <Font name="System Bold" size="18.0"/>
            </font>
        </Button>
        <AnchorPane fx:id="anchorPaneTinhNangNhanKhauCanBo" layoutX="758.0" prefHeight="603.0" prefWidth="157.0">
            <children>
                <Button fx:id="buttonDangXuatCanBo" layoutX="21.0" layoutY="7.0" mnemonicParsing="false"
                        onMousePressed="#onPressedButtonDangXuatCanBo" prefHeight="38.0" prefWidth="130.0"
                        text="Đăng xuất" textFill="WHITE">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Button>
                <Button fx:id="buttonPheDuyetThongTinDipDacBietCanBo" layoutX="21.0" layoutY="238.0"
                        mnemonicParsing="false" onMousePressed="#onPressedButtonPheDuyetThongTinDipDacBietCanBo"
                        prefHeight="38.0" prefWidth="130.0" text="Phê duyệt" textFill="WHITE">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Button>
                <Button fx:id="buttonTuChoiThongTinDipDacBietCanBo" layoutX="21.0" layoutY="308.0"
                        mnemonicParsing="false" onMousePressed="#onPressedButtonTuChoiThongTinDipDacBietCanBo"
                        prefHeight="38.0" prefWidth="130.0" text="Từ chối" textFill="WHITE">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Button>
                <Button fx:id="buttonThemMoiThongTinDipDacBietCanBo" layoutX="21.0" layoutY="168.0"
                        mnemonicParsing="false" onMousePressed="#onPressedButtonThemMoiThongTinDipDacBietCanBo"
                        prefHeight="38.0" prefWidth="130.0" text="Thêm mới" textFill="WHITE">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Button>
                <Button fx:id="buttonXoaThongTinDipDacBietCanBo" layoutX="21.0" layoutY="378.0" mnemonicParsing="false"
                        onMousePressed="#onPressedButtonXoaThongTinDipDacBietCanBo" prefHeight="38.0" prefWidth="130.0"
                        text="Xóa" textFill="WHITE">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Button>
                <Button fx:id="buttonThoatThongTinDipDacBietCanBo" layoutX="21.0" layoutY="448.0"
                        mnemonicParsing="false" onMousePressed="#onPressedButtonThoatThongTinDipDacBietCanBo"
                        prefHeight="38.0" prefWidth="130.0" text="Thoát" textFill="WHITE">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Button>
            </children>
        </AnchorPane>
        <Button fx:id="buttonGiaiThuongCanBo" layoutY="301.0" mnemonicParsing="false"
                onMousePressed="#onPressedButtonGiaiThuongCanBo" prefHeight="58.0" prefWidth="222.0" text="Giải Thưởng"
                textFill="WHITE">
            <graphic>
                <ImageView fitHeight="44.0" fitWidth="45.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@Icon/IconAdminAward.png"/>
                    </image>
                </ImageView>
            </graphic>
            <font>
                <Font name="System Bold" size="18.0"/>
            </font>
        </Button>
        <TableView fx:id="tableViewDipDacBietCanBo" layoutX="222.0" layoutY="80.0"
                   onKeyPressed="#onDeletePressedTrongBangThongTinDipDacBietCanBo" prefHeight="526.0" prefWidth="554.0">
            <columns>
                <TableColumn fx:id="tableColumnMaNhanKhauDipDacBietCanBo" onEditCancel="#handleOnEditCancel"
                             onEditCommit="#handleOnEditCommit" prefWidth="107.0" text="Mã nhân khẩu"/>
                <TableColumn fx:id="tableColumnHoTenDipDacBietCanBo" prefWidth="130.0" text="Họ tên"/>
                <TableColumn fx:id="tableColumnTenDipDacBietCanBo" prefWidth="136.0" text="Tên dịp"/>
                <TableColumn fx:id="tableColumnNamDipDacBietCanBo" prefWidth="54.0" text="Năm"/>
                <TableColumn fx:id="tableColumnTinhTrangDipDacBietCanBo" onEditCancel="#handleOnEditCancel"
                             onEditCommit="#handleOnEditCommit" prefWidth="126.0" text="Tình Trạng"/>
            </columns>
        </TableView>
        <AnchorPane layoutX="222.0" layoutY="18.0" onKeyPressed="#onEnterPressedTrongOTimKiemDipDacBietCanBo">
            <children>
                <TextField fx:id="textFieldLocThongTinDipDacBietCanBo" prefHeight="31.0" prefWidth="213.0"
                           promptText="Tìm kiếm theo tên">
                    <font>
                        <Font name="System Italic" size="15.0"/>
                    </font>
                </TextField>
                <ComboBox fx:id="comboBoxTimKiemDipDacBietCanBo" layoutX="224.0"
                          onAction="#onSelectionComboBoxTimKiemDipDacBietCanBo" prefHeight="31.0" prefWidth="100.0"/>
                <Button fx:id="buttonLocThongTinDipDacBietCanBo" layoutX="335.0" mnemonicParsing="false"
                        onMousePressed="#onPressedButtonLocThongTinDipDacBietCanBo" prefHeight="31.0" prefWidth="100.0"
                        text="Tìm kiếm" textFill="WHITE">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Button>
                <ComboBox fx:id="comboBoxTenNamDipDacBietCanBo" disable="true" prefHeight="31.0" prefWidth="213.0"/>
                <ComboBox fx:id="comboBoxMaNhanKhauDipDacBietCanBo" disable="true" prefHeight="31.0" prefWidth="213.0"/>
                <ComboBox fx:id="comboBoxTinhTrangDipDacBietCanBo" disable="true" prefHeight="31.0" prefWidth="213.0"/>
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
