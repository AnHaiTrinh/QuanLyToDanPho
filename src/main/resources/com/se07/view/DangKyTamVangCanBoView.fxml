                <Label text="Mã nhân khẩu" GridPane.columnIndex="1">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Label>
                <TextField fx:id="textFieldNoiTamVangCanBo" prefHeight="31.0" promptText="Bắt buộc"
                           GridPane.columnIndex="2" GridPane.rowIndex="1"/>
            </children>
        </GridPane>
        <Label layoutX="485.0" layoutY="81.0" text="Đăng ký tạm vắng">
            <font>
                <Font name="System Bold" size="20.0"/>
            </font>
        </Label>
