                <Label text="Lý do" GridPane.columnIndex="1" GridPane.rowIndex="2">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Label>
                <Label text="Họ tên" GridPane.columnIndex="4">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Label>
                <Label text="Từ ngày" GridPane.columnIndex="4" GridPane.rowIndex="1">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Label>
                <Label text="Đến ngày" GridPane.columnIndex="4" GridPane.rowIndex="2">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Label>
                <TextField fx:id="textFieldLyDoTamVangCanBo" prefHeight="31.0" promptText="Bắt buộc"
                           GridPane.columnIndex="2" GridPane.rowIndex="2"/>
                <TextField fx:id="textFieldHoTenTamVangCanBo" prefHeight="31.0" promptText="Bắt buộc"
                           GridPane.columnIndex="5"/>
                <DatePicker fx:id="datePickerDenNgayTamVangCanBo" prefHeight="31.0" prefWidth="264.0"
