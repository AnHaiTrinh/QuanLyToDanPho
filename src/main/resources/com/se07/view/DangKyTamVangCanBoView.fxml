                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Button>
                <Button fx:id="buttonHuyXacNhanDangKyTamVangCanBo" layoutX="4.0" layoutY="548.0" mnemonicParsing="false"
                        onMousePressed="#onPressedButtonHuyXacNhanDangKyTamVangCanBo" prefHeight="31.0" prefWidth="91.0"
                        text="Hủy">
                    <textFill>
                        <LinearGradient endX="1.0" endY="1.0">
                            <stops>
                                <Stop color="#fcfcfc"/>
                                <Stop color="WHITE" offset="1.0"/>
                            </stops>
                        </LinearGradient>
                    </textFill>
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Button>
                <Button fx:id="buttonXacNhanDangKyTamVangCanBo" layoutX="-132.0" layoutY="548.0" mnemonicParsing="false"
                        onMousePressed="#onPressedButtonXacNhanDangKyTamVangCanBo" prefHeight="31.0" prefWidth="91.0"
                        text="Xác nhận ">
                    <textFill>
                        <LinearGradient endX="1.0" endY="1.0">
                            <stops>
                                <Stop color="#fcfcfc"/>
                                <Stop color="WHITE" offset="1.0"/>
                            </stops>
                        </LinearGradient>
                    </textFill>
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Button>
            </children>
        </AnchorPane>
        <Button fx:id="buttonGiaiThuongCanBo" layoutY="301.0" mnemonicParsing="false"
                onMousePressed="#onPressedButtonGiaiThuongCanBo" prefHeight="58.0" prefWidth="222.0" text="Giải Thưởng"
